        }
        .content{
            /*margin-top:120px;*/
            /*padding-top:120px;*/
            background-color:var(--content-color);
            padding: 10px 0;
        }

        /*tai khoan*/
        .stardust-popover{
            border-radius:3px;
            position:absolute;
            background-color:var(--while-color);
            display:none;
            animation:HeaderNotifyGrowth ease-in 0.3s;
            transform-origin:20px top;
            z-index:999;
        }
        .header__navbar-item-taikhoan:hover .stardust-popover{
            display:block;
        }
            .stardust-popover::before {
                content: "";
                border-width: 10px 16px;
                border-style: solid;
                border-color: transparent transparent var(--while-color) transparent;
                left: 14px;
                position:absolute;
                top: -20px;
                z-index:0
            }

            .stardust-popover::after{
                content: "";
                display: block;
                position: absolute;
                width: 100px;
                height: 10px;
                left: 0px;
                top: -10px;
                z-index: 99;
            }

        .stardust-popover__list{
            list-style:none;
            padding:0;
        }
        .list-item{
            padding:12px 12px;
            font-size:1.4rem;
        }
        .list-item:hover,
        .btndangxuat:hover{
            background-color:rgba(0,0,0,0.05);
        }

        .list-item_link{
            text-decoration:none;
            color:var(--text-color);
        }
        .btndangxuat{
            font-size:1.4rem;
            text-align:left;
            padding:12px;
            width:100%;
            height:100%;
            background-color:var(--while-color);
            border:0px;
        }
        
        /*gio hang*/

